{
    "name": "voku/simple-php-code-parser",
    "description": "Get a simple data structure from your php code.",
    "type": "library",
    "keywords": [
        "php",
        "parser",
        "phpdoc"
    ],
    "homepage": "https://github.com/voku/Simple-PHP-Code-Parser",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lars Moelleken",
            "homepage": "http://www.moelleken.org/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-simplexml": "*",
        "amphp/parallel": "~1.4",
        "composer/composer": "~1.10",
        "phpdocumentor/type-resolver": "~1.0 || ~1.1",
        "phpdocumentor/reflection-docblock": "~4.3 || ~5.1",
        "phpdocumentor/reflection-common": "~2.1",
        "voku/simple-cache": "~4.0",
        "nikic/php-parser": "~4.4",
        "ondrejmirtes/better-reflection": "~4.3",
        "vimeo/psalm": "~3.11",
        "symfony/console": "~3.4 || ~4.1 || ~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0 || ~7.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "voku\\": "src/voku/"
        },
        "files": [
            "src/voku/SimplePhpParser/Model/types/ArrayArray.php",
            "src/voku/SimplePhpParser/Model/types/ArrayFloat.php",
            "src/voku/SimplePhpParser/Model/types/ArrayInt.php",
            "src/voku/SimplePhpParser/Model/types/ArrayString.php",
            "src/voku/SimplePhpParser/Model/types/BooleanFalse.php",
            "src/voku/SimplePhpParser/Model/types/BooleanTrue.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "voku\\tests\\": "tests/"
        }
    }
}
